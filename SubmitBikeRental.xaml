<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
     xmlns:UserControls ="clr-namespace:BikeRide.UserControls"

    x:Class="BikeRide.SubmitBikeRental"
        mc:Ignorable="d"
        Loaded = "Window_Loaded"
        Title="SubmitBikeRental" Height="600" Width="800">




    <Grid x:Name="Header" Margin="-5,0,5,0">

        <ToolBar HorizontalAlignment="Left" Height="68" VerticalAlignment="Top" Width="800"/>


        <Frame x:Name="SubmitBikeRentalFrame" NavigationUIVisibility="Hidden"  />

        <TextBlock x:Name="Heading" Margin="20" Text = "Submitting Bike rental" FontSize="20" FontWeight="Bold"   HorizontalAlignment="Center" VerticalAlignment="Top" TextWrapping="Wrap" />



        <!--! list and filter-->


    </Grid>
</Window>
